{
  "author": "igorlogius",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_area": "tabstrip"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Alt+I"
      }
    }
  },
  "description": "Invert the current tab selection",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Invert Tab Selection",
  "permissions": [
    "tabs",
    "menus"
  ],
  "version": "1.1.3"
}
